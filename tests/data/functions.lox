fun fib(limit) {
  var a = 0;
  var temp;

  for (var b = 1; a < limit; b = temp + b) {
    print a;
    temp = a;
    a = b;
  }
}

fib(10000);

fun add(a, b) {
  return a + b;
}

assert(add(1, 2) == 3, "add function should work correctly");

fun rfib(n) {
  if (n <= 1) return n;
  return rfib(n - 2) + rfib(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
  print rfib(i);
}